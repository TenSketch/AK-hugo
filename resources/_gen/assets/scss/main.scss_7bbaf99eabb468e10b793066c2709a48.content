/* Custom CSS for sidebar */
#sidebar-right {
  position: fixed;
  top: 0;
  right: -300px;
  /* Initially hidden */
  width: 300px;
  height: 100%;
  background-color: #FFEFE6;
  transition: all 0.3s ease-in-out;
  z-index: 1050;
  box-shadow: -2px 0 5px rgba(0, 0, 0, 0.1); }

#sidebar-right.show {
  right: 0; }

/* Overlay effect when sidebar is open */
.sidebar-overlay {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  z-index: 1040;
  display: none; }

.sidebar-overlay.show {
  display: block;
  animation: fadeIn 0.3s ease-in-out; }

.linksa {
  margin-top: 1px;
  border-radius: 0.2rem;
  color: #2A5D34; }

.linksa:hover {
  background-color: #B8E6C1;
  color: #2A5D34; }

/* Animations */
@keyframes fadeIn {
  from {
    opacity: 0; }
  to {
    opacity: 1; } }

@keyframes slideIn {
  from {
    right: -300px; }
  to {
    right: 0; } }

footer {
